.data

.text

main:
    li $v0, 5
    syscall
    move $s0, $v0
    li $s1, 0
    li $s6, 2

loop: 
    beq $s1, $s0, termina
    addi $s1, $s1, 1
    li $v0, 8
    syscall
    move $t0, $v0
    sw $t1, 0($t0)
    sw $t2, 2($t0)
    beq $t1, $t2, ret1
    li $v0, 1
    move $a0, $s6
    syscall
    j loop


termina:
    li $v0, 10
    syscall

ret1: 
    li $s7, 1
    li $v0, 1
    move $a0, $s7
    syscall
    j loop
